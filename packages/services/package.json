{
  "name": "@ficommerce/services",
  "version": "1.0.0",
  "description": "",
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "module": "dist/esm5/index.js",
  "es2015": "dist/esm2015/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "types": {
        "default": "./dist/types/index.d.ts"
      },
      "import": "./dist/esm5/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./constants": {
      "types": {
        "default": "./dist/types/constants/index.d.ts"
      },
      "import": "./dist/esm5/constants/index.js",
      "require": "./dist/cjs/constants/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "npm run packages",
    "build:dev": "npm-run-all --parallel build:*:dev ",
    "build:cjs:dev": "tsc -w --project  tsconfig.cjs.json",
    "build:cjs": "tsc --project  tsconfig.cjs.json",
    "build:types": "tsc --project  tsconfig.types.json",
    "build:types:dev": "tsc -w --project  tsconfig.types.json",
    "build:esm5": "tsc --project  tsconfig.esm5.json",
    "build:esm2015": "tsc --project  tsconfig.esm2015.json",
    "packages": "npm-run-all \" build:!(dev) \"",
    "start:dev": "npm run build:dev",
    "preinstall": "pnpm run packages",
    "test": "jest"
  },
  "dependencies": {
    "@ficommerce/generated": "workspace:*",
    "@ficommerce/shared": "workspace:*",
    "@types/graphql-fields": "^1.3.5",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "bullmq": "^1.85.3",
    "class-validator": "0.14.0",
    "graphql": "16.7.1",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.1",
    "node-fetch": "^3.3.1",
    "reflect-metadata": "0.1.13",
    "squel": "^5.13.0",
    "stripe": "12.14.0",
    "tslib": "^2.6.0",
    "type-graphql": "2.0.0-beta.2",
    "typegraphql-prisma": "^0.26.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "20.4.1",
    "@types/node-fetch": "^2.6.4",
    "jest": "^29.6.2",
    "npm-run-all": "^4.1.5",
    "prisma": "4.15.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
