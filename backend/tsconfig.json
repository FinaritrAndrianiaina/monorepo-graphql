{
  "compilerOptions": {
    "outDir": "dist",
    "lib": ["es2020","esnext.asynciterable"],
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node16",
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "types": ["node"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": "./",
    "paths": {
      "services/*": ["./services/*"],
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": ["node_modules"]
}